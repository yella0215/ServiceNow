        <name>Copy Incident</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[(function executeAction() {
    // Get the current incident record
    var currentIncident = current;

    // Create a new incident record
    var newIncident = new GlideRecord('	x_1552560_training_incident_new');
    newIncident.initialize();  // Initialize the new record to copy values

    // Copy fields from the current incident to the new one
    newIncident.short_description = currentIncident.short_description;
    newIncident.description = currentIncident.description;
    newIncident.category = currentIncident.category;
    newIncident.subcategory = currentIncident.subcategory;
    newIncident.priority = currentIncident.priority;
    newIncident.impact = currentIncident.impact;
    newIncident.urgency = currentIncident.urgency;
    newIncident.assignment_group = currentIncident.assignment_group;
    newIncident.assigned_to = currentIncident.assigned_to;
    newIncident.state = 1;
    
    // Optionally copy other fields, like custom fields, as necessary

    // Insert the new record (this creates the duplicate incident)
    var newIncidentSysId = newIncident.insert();

    // Open the newly created incident
    if (newIncidentSysId) {
        gs.addInfoMessage('Incident copied successfully. New Incident Sys ID: ' + newIncidentSysId);
        // Optionally, redirect to the new incident record
        action.setRedirectURL(newIncident);
    } else {
        gs.addErrorMessage('Failed to copy the incident.');
    }
})(current);
]]></script>
